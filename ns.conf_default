; Own name servers
ns[] = dns1.example.com
ns[] = dns2.example.com
ns[] = dns3.example.com

[nsupdate]
name[internal] = dnsinternal.example.com
name[external] = dns1.example.com
TTL = 86400
; absolute path of nsupdate key (-k opt)
;key = /var/named/chroot/etc/pki/dnssec-keys/client.key
;If you don't use auth by key, then uncomment the follow line
;key =
